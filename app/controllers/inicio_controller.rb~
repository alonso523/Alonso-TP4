class InicioController < ApplicationController

   def index
    respond_to do |format|
      format.html # index.html.erb    
      format.json { render 'application.html.erb' }
    end
  end

  def menu
   respond_to do |format|
      format.html # index.html.erb    
      format.json { render :template => "inicio/menu" }
    end
  end
end
